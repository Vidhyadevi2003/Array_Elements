#include<iostream>
using namespace std;
int main()
{
    int n,i;
    cout<<"Enter a Number:";
    cin>>n;
    int a[n];
    cout<<"Enter elements:";
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    cout<<"You Entered:";
    for(i=0;i<n;i++)
    {
        cout<<a[i]<<"\n";
    }
    return 0;
}


